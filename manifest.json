{
"name":"WhatsappTwitch",
"version":"1.0",
"manifest_version":2,
"permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "chrome://favicon/",
    "storage"
],

"web_accessible_resources": ["src/bakedEmotes.js"],
"content_scripts": [
    {
      "matches": ["https://web.whatsapp.com/*"],
      "js": ["/src/script.js"]
    }
  ]
}
